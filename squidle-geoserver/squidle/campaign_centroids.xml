<?xml version="1.0"?>
<filters>
    <filter>
        <name>TIME</name>
        <type>datetime</type>
        <label>Date (UTC)</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
        <wmsStartDateName>date</wmsStartDateName>
        <wmsEndDateName>date</wmsEndDateName>        
    </filter>
    <filter>
        <name>platform_names</name>
        <type>string</type>
        <label>Platform Names</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>media_count</name>
        <type>long</type>
        <label>Media Count</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>total_annotation_count</name>
        <type>long</type>
        <label>Total Annotation Count</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>public_annotation_count</name>
        <type>long</type>
        <label>Public Annotation Count</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>centroid</name>
        <type>geometrypropertytype</type>
        <label>Geom</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>    
</filters>
